<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated toyLanguage -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.unice.polytech.si5.toylanguage.abstractsyntax.toyLanguage.presentation.ToyLanguageModelWizardID"
            name="%_UI_ToyLanguageModelWizard_label"
            class="fr.unice.polytech.si5.toylanguage.abstractsyntax.toyLanguage.presentation.ToyLanguageModelWizard"
