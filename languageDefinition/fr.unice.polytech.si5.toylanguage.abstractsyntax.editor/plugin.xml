   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated toyLanguage -->
      <editor
            id="fr.unice.polytech.si5.toylanguage.abstractsyntax.toyLanguage.presentation.ToyLanguageEditorID"
            name="%_UI_ToyLanguageEditor_label"
            icon="icons/full/obj16/ToyLanguageModelFile.gif"
            extensions="toylanguage"
            class="fr.unice.polytech.si5.toylanguage.abstractsyntax.toyLanguage.presentation.ToyLanguageEditor"
            contributorClass="fr.unice.polytech.si5.toylanguage.abstractsyntax.toyLanguage.presentation.ToyLanguageActionBarContributor">
